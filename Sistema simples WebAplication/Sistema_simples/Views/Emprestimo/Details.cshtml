@model Sistema_simples.Models.Emprestimo

<div>
    <h4>Emprestimo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            cod.Funcionario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsuarioId)
        </dd>
        <dt class = "col-sm-2">
            Nome do Funcionario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UsuarioNome)
        </dd>
        <dt class = "col-sm-2">
            cod.Cliente
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ClienteId)
        </dd>
        <dt class = "col-sm-2">
            cod.Produto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProdutoId)
        </dd>
        <dt class = "col-sm-2">
            Quantidade
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.quantidade)
        </dd>
        <dt class = "col-sm-2">
            Dt.Emprestimo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.data)
        </dd>
        <dt class = "col-sm-2">
            Dt.Devolução
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.data_devolucao)
        </dd>
        <dt class = "col-sm-2">
            Status
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.status)
        </dd>
        <dt class = "col-sm-2">
            Informações adicionais
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.informacoes_adicionais)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" asp-route-usuarioId="@ViewData["id"]" class="btn btn-success">Editar</a>
    <a asp-action="Index" asp-route-usuarioId="@ViewData["id"]" class="btn btn-secondary">Back to List</a>
</div>
